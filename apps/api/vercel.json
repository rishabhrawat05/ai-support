{
  "buildCommand": "npx prisma generate --schema=prisma/schema.prisma",
  "outputDirectory": "public",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "maxDuration": 60,
      "includeFiles": "src/**,prisma/**,node_modules/.prisma/**,node_modules/@prisma/**"
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api" }
  ]
}
