{
  "buildCommand": "npx prisma generate --schema=prisma/schema.prisma && mkdir -p public",
  "outputDirectory": "public",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    { "source": "/(.*)", "destination": "/api" }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 60
    }
  }
}
